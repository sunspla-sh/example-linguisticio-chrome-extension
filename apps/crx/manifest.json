{
  "manifest_version": 3,
  "name": "CRXJS Svelte Vite Example",
  "version": "1.0.0",
  "action": { "default_popup": "index.html" },
  "icons": {
    "16": "linguisticio-16.png",
    "32": "linguisticio-32.png",
    "48": "linguisticio-48.png",
    "128": "linguisticio-128.png"
  },
  "web_accessible_resources": [
    {
      "resources": [
        "Lato-Black.ttf",
        "Lato-BlackItalic.ttf",
        "Lato-Bold.ttf",
        "Lato-BoldItalic.ttf",
        "Lato-Regular.ttf",
        "Lato-Italic.ttf",
        "Lato-Light.ttf",
        "Lato-LightItalic.ttf",
        "Lato-Thin.ttf",
        "Lato-ThinItalic.ttf"
      ],
      "matches": ["<all_urls>"]
    }
  ]

}